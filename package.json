{
  "name": "react-environment-check",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">= 14.0.0"
  },
  "dependencies": {
    "react-scripts": "4.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.49.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
